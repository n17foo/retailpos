{
  "name": "retail-pos",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android --non-interactive",
    "ios": "expo start --ios --clear",
    "web": "expo start --web",
    "desktop": "NODE_ENV=development concurrently \"expo start --web --port 19006\" \"wait-on http://localhost:19006 && electron electron/main.js\"",
    "desktop:dev": "NODE_ENV=development electron electron/main.js",
    "desktop:build": "expo export --platform web && electron-builder --publish=never",
    "desktop:build-win": "expo export --platform web && electron-builder --win --publish=never",
    "desktop:build-mac": "expo export --platform web && electron-builder --mac --publish=never",
    "desktop:build-linux": "expo export --platform web && electron-builder --linux --publish=never",
    "test": "jest --no-coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "latest": "yarn upgrade --latest && npx expo install --fix",
    "doctor": "npx expo-doctor",
    "lint": "npx tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npx tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:errors": "npx tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "7.14.0",
    "@react-navigation/native": "7.1.28",
    "@react-navigation/native-stack": "7.13.0",
    "@react-navigation/stack": "7.7.2",
    "@stripe/stripe-terminal-react-native": "0.0.1-beta.28",
    "@tillpos/rn-receipt-printer-utils": "1.0.2",
    "@worldpay/access-worldpay-checkout-react-native-sdk": "4.0.2",
    "eventemitter3": "5.0.4",
    "expo": "54.0.33",
    "expo-camera": "17.0.10",
    "expo-localization": "17.0.8",
    "expo-sqlite": "16.0.10",
    "expo-status-bar": "3.0.9",
    "i18next": "25.8.13",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "16.5.4",
    "react-native": "0.81.5",
    "react-native-ble-plx": "3.5.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-keychain": "10.0.0",
    "react-native-logs": "5.5.0",
    "react-native-nitro-modules": "^0.33.9",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-square-in-app-payments": "^2.0.0",
    "react-native-vision-camera": "4.7.3",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "react-native-worklets-core": "^1.6.3",
    "uuid": "^13.0.0",
    "winston": "3.19.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@babel/core": "7.29.0",
    "@eslint/js": "10.0.1",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.10",
    "@types/react-native-square-in-app-payments": "1.4.4",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "@typescript-eslint/parser": "8.56.0",
    "concurrently": "^9.2.1",
    "electron": "^40.6.0",
    "electron-builder": "^26.8.1",
    "eslint": "10.0.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-native": "5.0.0",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "lint-staged": "^16.2.7",
    "prettier": "3.8.1",
    "react-native-dotenv": "3.4.11",
    "ts-jest": "^29.4.6",
    "typescript": "5.9.3",
    "wait-on": "^9.0.4"
  },
  "homepage": "./",
  "build": {
    "appId": "com.retail-pos.desktop",
    "productName": "RetailPOS",
    "directories": {
      "output": "dist-electron"
    },
    "extends": null,
    "files": [
      "electron/**/*",
      "dist/**/*",
      "package.json"
    ],
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "mac": {
      "category": "public.app-category.business",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "extraResources": [
      {
        "from": "dist",
        "to": "dist",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@stripe/stripe-terminal-react-native",
          "@tillpos/rn-receipt-printer-utils",
          "@worldpay/access-worldpay-checkout-react-native-sdk",
          "react-native-square-in-app-payments",
          "react-native-esc-pos-printer"
        ]
      }
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
