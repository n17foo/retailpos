{
  "name": "retail-pos",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android --non-interactive",
    "ios": "expo start --ios --clear",
    "web": "expo start --web",
    "desktop": "NODE_ENV=development concurrently \"expo start --web --port 19006\" \"wait-on http://localhost:19006 && electron electron/main.js\"",
    "desktop:dev": "NODE_ENV=development electron electron/main.js",
    "desktop:build": "expo export --platform web && electron-builder",
    "desktop:build-win": "expo export --platform web && electron-builder --win",
    "desktop:build-mac": "expo export --platform web && electron-builder --mac",
    "desktop:build-linux": "expo export --platform web && electron-builder --linux",
    "test": "jest --watchAll",
    "latest": "yarn upgrade --latest && npx expo install --fix",
    "doctor": "npx expo-doctor",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "7.9.0",
    "@react-navigation/native": "7.1.26",
    "@react-navigation/native-stack": "7.9.0",
    "@react-navigation/stack": "7.6.13",
    "@stripe/stripe-terminal-react-native": "0.0.1-beta.28",
    "@tillpos/rn-receipt-printer-utils": "1.0.2",
    "@worldpay/access-worldpay-checkout-react-native-sdk": "4.0.1",
    "eventemitter3": "5.0.1",
    "expo": "54.0.30",
    "expo-camera": "17.0.10",
    "expo-localization": "17.0.8",
    "expo-sqlite": "16.0.10",
    "expo-status-bar": "3.0.9",
    "i18next": "25.7.3",
    "react": "19.1.0",
    "react-i18next": "16.5.0",
    "react-native": "0.81.5",
    "react-native-ble-plx": "3.5.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keychain": "10.0.0",
    "react-native-logs": "5.5.0",
    "react-native-mmkv": "4.1.0",
    "react-native-nitro-modules": "^0.32.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-square-in-app-payments": "^1.7.6",
    "react-native-vision-camera": "4.7.3",
    "react-native-worklets": "0.5.1",
    "react-native-worklets-core": "^1.6.2",
    "winston": "3.19.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@types/react": "~19.1.10",
    "@types/react-native-square-in-app-payments": "1.4.4",
    "@typescript-eslint/eslint-plugin": "8.50.1",
    "@typescript-eslint/parser": "8.50.1",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-native": "5.0.0",
    "prettier": "3.7.4",
    "react-native-dotenv": "3.4.11",
    "typescript": "5.9.3",
    "wait-on": "^9.0.3"
  },
  "homepage": "./",
  "build": {
    "appId": "com.retail-pos.desktop",
    "productName": "RetailPOS",
    "directories": {
      "output": "dist-electron"
    },
    "extends": null,
    "files": [
      "electron/**/*",
      "dist/**/*",
      "package.json"
    ],
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "mac": {
      "category": "public.app-category.business",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "extraResources": [
      {
        "from": "dist",
        "to": "dist",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
